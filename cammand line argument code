
import java.util.Vector;

class Demo {
    public static void main(String[] args) {
        Vector v = new Vector();

        for (int i = 0; i < args.length; i++) {
            int num = Integer.valueOf(args[i]);  // convert argument to int
            v.add(num);  // add to vector
        }

        System.out.println("Vector contains: " + v);
    }
}

import java.util.Vector;

class Demo {
  
    public static void main(String[] args) {
      int count=0;
        Vector v = new Vector();

        for (int i = 0; i < args.length; i++) {
            int num = Integer.valueOf(args[i]); 
            count++ ;// convert argument to int
            v.add(num);  // add to vector
        }

        System.out.println("Vector contains: " + v);
        System.out.println("size of argument  is "+ count);
    }
}
